{
    "routes": [
        {
            "src": "/api/*",
            "methods": [
                "GET",
                "HEAD"
            ],
            "headers": {
                "Access-Control-Allow-Origin": "*"
            }
        }
    ],
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "index.js"
        },
        {
            "src": "/(.*)",
            "dest": "public/$1"
        }
    ]
}